{
  "extends": "ts-config-single-spa",
  "compilerOptions": {
    "jsx": "react"
  },
  "include": ["src/**/*", "../@types"],
  "alwaysStrict": true,
  "noFallthroughCasesInSwitch": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
}
